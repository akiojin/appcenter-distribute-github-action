{
  "name": "appcenter-distribute-github-action",
  "version": "1.4.10",
  "description": "This action uploads artefacts (.apk or .ipa) to App Center.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm install && ncc build src/main.ts --license licenses.txt",
    "clean": "rimraf dist && rimraf node_modules && npm cache clean --force"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akiojin/appcenter-distribute-github-action.git"
  },
  "keywords": [
    "AppCenter",
    "Distribution",
    "CI"
  ],
  "author": "Akio Jinsenji <akio-jinsenji@cloud-creative-studios.com> (https://github.com/akiojin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akiojin/appcenter-distribute-github-action/issues"
  },
  "homepage": "https://github.com/akiojin/appcenter-distribute-github-action#readme",
  "dependencies": {
    "tmp": "0.2.1"
  },
  "devDependencies": {
    "@actions/core": "1.10.1",
    "@actions/exec": "1.1.1",
    "@actions/github": "6.0.0",
    "@akiojin/argument-builder": "1.0.0",
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.4.3",
    "@types/node": "20.11.15",
    "@types/tmp": "0.2.6",
    "@vercel/ncc": "0.38.1",
    "husky": "8.0.3",
    "rimraf": "5.0.5",
    "typescript": "5.3.3"
  },
  "private": true
}
